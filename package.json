  },
  "config": {
    "port": 3000
  },
  "devDependencies": {
    "eslint": "^3.15.0",
    "glob": "^7.1.1",
    "nodemon": "^1.11.0",
    "standard": "^8.6.0",
    "systemjs": "^0.20.5",
    "systemjs-builder": "^0.16.2"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.22.0",
    "cookie-parser": "^1.4.3",
    "document-register-element": "^1.3.0",
    "es6-promise": "^4.0.5",
    "express": "^4.14.1",
    "morgan": "^1.8.0",
    "pug": "^2.0.0-beta11",
    "systemjs-plugin-babel": "^0.0.20",
    "uuid": "^3.0.1",
    "whatwg-fetch": "^2.0.2",
    "yargs": "6.6.0",
    "zepto": "^1.2.0"
  }
}
